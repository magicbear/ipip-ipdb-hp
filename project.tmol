build-backend = "setuptools.build_meta"

[project]
name = "ipip-ipdb-hp"
dependencies = [
]
dynamic = ["version"]

[tool.setuptools]
package-dir = {"" = "src"}

[tool.setuptools.packages.find]
where = ["src"]

[project.urls]
